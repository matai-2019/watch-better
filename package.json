{
  "name": "watch_better",
  "version": "0.1.0",
  "description": "Watch Better - A Diverse Movie Database",
  "repository": "https://github.com/matai-2019/watch-better.git",
  "private": false,
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "dependencies": {
    "dotenv": "^8.0.0",
    "dotenv-webpack": "^1.7.0",
    "express": "^4.17.1",
    "express-jwt": "^5.3.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^2.2.0",
    "knex": "^0.17.6",
    "libsodium-wrappers-sumo": "^0.7.4",
    "lodash": "^4.17.21",
    "node-env-run": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "pg": "^7.12.0",
    "superagent": "^5.1.0"
  },
  "scripts": {
    "start": "node server",
    "dev": "run-p dev-*",
    "dev-server": "nodemon --watch server",
    "dev-watch": "webpack --watch",
    "test": "jest --verbose=true --silent",
    "test:server": "jest tests/server --watchAll",
    "test:src": "jest tests/src --watchAll",
    "knex": "knex --knexfile server/db/knexfile.js",
    "db-reset": "run-s db:*",
    "db:delete": "rm -f server/db/dev.sqlite3",
    "db:migrate": "npm run knex migrate:latest",
    "db:seed": "npm run knex seed:run",
    "server": "nodemon server | pino-colada",
    "postinstall": "run-s db:migrate db:seed"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@material-ui/core": "^4.1.2",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.20",
    "authenticare": "^0.4.1",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "cheerio": "^1.0.0-rc.3",
    "css-loader": "^3.2.0",
    "downshift": "^3.2.10",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.2",
    "eslint-plugin-standard": "^4.0.0",
    "formik": "^1.5.8",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-alice-carousel": "^1.15.2",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "react-router-dom": "^5.0.1",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^4.0.9",
    "style-loader": "^1.0.0",
    "supertest": "^4.0.2",
    "webpack": "^4.39.1",
    "webpack-cli": "^3.3.6",
    "yup": "^0.27.0"
  }
}
